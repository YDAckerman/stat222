options(stringsAsFactors = FALSE)

.First <- function(){

    quietLoad <- function(lib){
        suppressPackageStartupMessages(
            library(lib, character.only = TRUE)
        )
    }
    
    libs <- c("plyr","dplyr", "ggplot2","reshape2",
              "readxl", "pwr", "broom", "httr",
              "foreign", "helperFunctions", "rvest",
              ## "Hmisc",
              "psychometric")

    sapply(libs, quietLoad)
    rm(libs, quietLoad)

    cat("\nHello!\n") 
}

.Last <- function(){
    
    cat("\nGoodbye!\n")
    
}
